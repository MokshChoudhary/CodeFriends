#include "database_helper.h"

int db_provider::count_data_callback(void *data, int argc, char **argv, char **azColName){

    data = &argc;
    return 0;

}

int db_provider::count_data(sqlite3 *db, std::string query){

    int rc = 0;
    const int* data;
    char* error = nullptr;
    rc = sqlite3_exec(db, query.c_str(), count_data_callback, (int*)data, &error);
    if(rc != SQLITE_OK){
        return rc;
    }else{
        return rc;
    }
}
